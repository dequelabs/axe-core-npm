{
  "name": "@axe-core/webdriverio",
  "version": "4.4.4",
  "description": "Provides a method to inject and analyze web pages using axe",
  "contributors": [
    {
      "name": "Michael Siek (me@michaelsiek.com)"
    }
  ],
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dequelabs/axe-core-npm.git"
  },
  "license": "MPL-2.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc-silent -p tsconfig.json --suppress @",
    "test": "mocha --timeout 60000 -r ts-node/register 'src/test.ts'",
    "coverage": "nyc npm run test",
    "prepare": "npm run build"
  },
  "keywords": [
    "a11y",
    "unit",
    "testing",
    "tdd",
    "bdd",
    "accessibility",
    "axe",
    "webdriver",
    "webdriverio"
  ],
  "dependencies": {
    "axe-core": "^4.4.2",
    "cssesc": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/chromedriver": "^81.0.1",
    "@types/cssesc": "^3.0.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.3",
    "@types/test-listen": "^1.1.0",
    "@wdio/sync": "^7.16.3",
    "axe-test-fixtures": "github:dequelabs/axe-test-fixtures#v1",
    "chai": "^4.3.6",
    "chromedriver": "^104.0.0",
    "delay": "^5.0.0",
    "express": "^4.18.1",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "test-listen": "^1.1.0",
    "ts-node": "^10.8.2",
    "tsc-silent": "^1.2.1",
    "typescript": "^4.7.4",
    "webdriverio": "^7.19.7"
  },
  "peerDependencies": {
    "webdriverio": "^5 || ^6 || ^7"
  },
  "nyc": {
    "include": [
      "src/index.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "checkCoverage": true,
    "statements": 85,
    "branches": 85,
    "functions": 85,
    "lines": 85
  }
}
