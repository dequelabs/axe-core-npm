{
  "name": "@axe-core/webdriverio",
  "version": "4.1.1",
  "description": "Provides a method to inject and analyze web pages using axe",
  "contributors": [
    {
      "name": "Michael Siek (me@michaelsiek.com)"
    }
  ],
  "license": "MPL-2.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc-silent -p tsconfig.json --suppress @",
    "test": "mocha --timeout 60000 -r ts-node/register 'src/test.ts'",
    "coverage": "nyc npm run test"
  },
  "keywords": [
    "a11y",
    "unit",
    "testing",
    "tdd",
    "bdd",
    "accessibility",
    "axe",
    "webdriver",
    "webdriverio"
  ],
  "dependencies": {
    "axe-core": "^4.1.2",
    "cssesc": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/chromedriver": "^81.0.0",
    "@types/cssesc": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/is-ci": "^2.0.0",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.25",
    "@types/sinon": "^9.0.10",
    "@types/test-listen": "^1.1.0",
    "@wdio/sync": "^6.11.0",
    "chai": "^4.3.0",
    "chromedriver": "^88.0.0",
    "delay": "^5.0.0",
    "express": "^4.17.1",
    "is-ci": "^2.0.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4",
    "source-map-support": "^0.5.19",
    "test-listen": "^1.1.0",
    "ts-node": "^9.1.1",
    "tsc-silent": "^1.2.1",
    "typescript": "^4.1.4",
    "webdriverio": "^6.12.1"
  },
  "peerDependencies": {
    "webdriverio": "^6.0.0 || ^5.0.0"
  },
  "nyc": {
    "include": [
      "src/index.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "checkCoverage": true,
    "statements": 100,
    "branches": 90,
    "functions": 100,
    "lines": 100
  }
}
