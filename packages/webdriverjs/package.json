{
  "name": "@axe-core/webdriverjs",
  "version": "4.7.3",
  "description": "Provides a method to inject and analyze web pages using axe",
  "contributors": [
    {
      "name": "Dylan Barrell",
      "organization": "Deque Systems, Inc.",
      "url": "http://github.com/dylanb/"
    },
    {
      "name": "Marcy Sutton",
      "organization": "Deque Systems, Inc.",
      "url": "http://github.com/marcysutton/"
    },
    {
      "name": "Wilco Fiers",
      "organization": "Deque Systems, Inc.",
      "url": "http://github.com/wilcofiers/"
    },
    {
      "name": "David Sturley",
      "organization": "Deque Systems, Inc.",
      "url": "http://deque.com/"
    },
    {
      "name": "Michael Siek (me@michaelsiek.com)"
    }
  ],
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dequelabs/axe-core-npm.git"
  },
  "license": "MPL-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "test": "mocha --timeout 60000 -r ts-node/register 'test/**.spec.ts'",
    "test:esm": "node test/esmTest.mjs",
    "coverage": "nyc npm run test",
    "prepare": "npm run build"
  },
  "keywords": [
    "a11y",
    "unit",
    "testing",
    "tdd",
    "bdd",
    "accessibility",
    "axe",
    "selenium",
    "webdriver",
    "webdriverjs"
  ],
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/chromedriver": "^81.0.1",
    "@types/express": "^4.17.14",
    "@types/http-server": "^0.12.1",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.8.3",
    "@types/selenium-webdriver": "^4.1.5",
    "@types/sinon": "^10.0.13",
    "@types/test-listen": "^1.1.0",
    "axe-test-fixtures": "github:dequelabs/axe-test-fixtures#v1",
    "chai": "^4.3.6",
    "chaimocha": "^1.10.0",
    "chromedriver": "^113.0.0",
    "delay": "^5.0.0",
    "express": "^4.18.2",
    "expresss": "^0.0.0",
    "http-server": "^14.1.1",
    "is-ci": "^3.0.1",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "selenium-webdriver": "^4.8.1",
    "sinon": "^14.0.1",
    "test-listen": "^1.1.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axe-core": "^4.7.0"
  },
  "peerDependencies": {
    "selenium-webdriver": ">3.0.0-beta  || >=2.53.1 || >4.0.0-alpha"
  },
  "nyc": {
    "include": [
      "src/index.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "checkCoverage": true,
    "statements": 85,
    "branches": 85,
    "functions": 85,
    "lines": 85
  }
}
