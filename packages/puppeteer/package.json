{
  "name": "@axe-core/puppeteer",
  "version": "4.3.2",
  "description": "Provides a chainable axe API for Puppeteer and automatically injects into all frames",
  "repository": {
    "type": "git",
    "url": "https://github.com/dequelabs/axe-core-npm.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "mocha",
    "coverage": "nyc mocha",
    "prepublishOnly": "npm run build"
  },
  "author": "Adam Cutler",
  "license": "MPL-2.0",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/express": "^4.17.13",
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.20.36",
    "@types/puppeteer": "^5.4.4",
    "@types/sinon": "^7.0.13",
    "@types/test-listen": "^1.1.0",
    "axe-test-fixtures": "github:dequelabs/axe-test-fixtures#v1",
    "chai": "^4.3.4",
    "express": "^4.16.4",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "puppeteer": "^13.1.3",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.20",
    "test-listen": "^1.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axe-core": "^4.4.1"
  },
  "peerDependencies": {
    "puppeteer": ">=1.10.0 <= 13"
  },
  "engines": {
    "node": ">=6.4.0"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/browser.ts"
    ],
    "all": true,
    "instrument": true,
    "sourceMap": true,
    "reporter": [
      "text-summary",
      "html"
    ],
    "statements": 85,
    "branches": 85,
    "functions": 85,
    "lines": 85
  }
}
