{
  "name": "@axe-core/cli",
  "version": "4.2.0",
  "description": "A CLI for accessibility testing using axe-core",
  "author": {
    "name": "Wilco Fiers",
    "organization": "Deque Systems, Inc.",
    "url": "http://github.com/wilcofiers/"
  },
  "contributors": [
    {
      "name": "Michael Siek (me@michaelsiek.com)"
    }
  ],
  "license": "MPL-2.0",
  "main": "dist/src/lib/index.js",
  "typings": "dist/src/types.js",
  "bin": {
    "axe": "./dist/src/bin/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "mocha --timeout 60000 -r ts-node/register 'src/**/**.test.ts'",
    "coverage": "nyc npm run test",
    "prepare": "npm run build"
  },
  "keywords": [
    "axe-core",
    "accessibility",
    "a11y",
    "wcag",
    "cli",
    "testing"
  ],
  "dependencies": {
    "@axe-core/webdriverjs": "^4.2.0",
    "axe-core": "^4.2.0",
    "chromedriver": "^90.0.0",
    "colors": "^1.4.0",
    "commander": "^7.1.0",
    "selenium-webdriver": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/chromedriver": "^81.0.0",
    "@types/mocha": "^8.2.1",
    "@types/mock-fs": "^4.13.0",
    "@types/selenium-webdriver": "^4.0.11",
    "chai": "^4.3.0",
    "execa": "^5.0.0",
    "mocha": "^8.3.0",
    "mock-fs": "^4.14.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "tempy": "^1.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "nyc": {
    "checkCoverage": true,
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "statements": 95,
    "branches": 93,
    "functions": 100,
    "lines": 95,
    "exclude": [
      "dist",
      "coverage",
      "**/*.test.ts",
      "src/testutils"
    ]
  }
}
