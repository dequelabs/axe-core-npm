{
  "name": "@axe-core/cli",
  "version": "4.7.0",
  "description": "A CLI for accessibility testing using axe-core",
  "author": {
    "name": "Wilco Fiers",
    "organization": "Deque Systems, Inc.",
    "url": "http://github.com/wilcofiers/"
  },
  "contributors": [
    {
      "name": "Michael Siek (me@michaelsiek.com)"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dequelabs/axe-core-npm.git"
  },
  "license": "MPL-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "axe": "./dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsup src/lib/index.ts --dts --format esm,cjs && tsup src/bin/cli.ts --format cjs",
    "test": "mocha --timeout 60000 -r ts-node/register 'src/**/**.test.ts'",
    "test:esm": "node esmTest.mjs",
    "coverage": "nyc npm run test",
    "prepare": "npm run build"
  },
  "keywords": [
    "axe-core",
    "accessibility",
    "a11y",
    "wcag",
    "cli",
    "testing"
  ],
  "dependencies": {
    "@axe-core/webdriverjs": "^4.7.0",
    "axe-core": "^4.7.0",
    "chromedriver": "^112.0.0",
    "colors": "^1.4.0",
    "commander": "^9.4.1",
    "selenium-webdriver": "^4.8.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/chromedriver": "^81.0.1",
    "@types/mocha": "^10.0.0",
    "@types/mock-fs": "^4.13.1",
    "@types/selenium-webdriver": "^4.1.5",
    "chai": "^4.3.6",
    "execa": "5.1.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "tempy": "1.0.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^4.8.4"
  },
  "nyc": {
    "checkCoverage": true,
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "statements": 95,
    "branches": 92,
    "functions": 94,
    "lines": 95,
    "exclude": [
      "dist",
      "coverage",
      "**/*.test.ts",
      "src/testutils"
    ]
  }
}
